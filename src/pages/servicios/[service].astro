---
export interface Props {
    service: Number;
}

export function getStaticPaths() {
    return [
        { params: { service: 1 } },
        { params: { service: 2 } },
        { params: { service: 3 } },
        { params: { service: 4 } },
    ];
}

const { service } = Astro.params;

import Layout from "../../layouts/Layout.astro";

import servicios from "../../seeds/servicios";
const servicio = servicios.find(servicio => Number(servicio.id) == Number(service))
---

<Layout title="SIOTEC - Servicios">
    {servicio.title}
</Layout>
